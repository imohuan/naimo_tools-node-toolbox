{
  "name": "node-toolbox",
  "version": "1.0.0",
  "description": "Node 工具箱 - 一站式 Node.js 开发环境管理工具",
  "type": "module",
  "scripts": {
    "init": "pnpm i && pnpm run add-electron-types && pnpm run dev",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "add-electron-types": "$env:ELECTRON_SKIP_DOWNLOAD=1; pnpm install electron --save-dev",
    "deploy": "node scripts/deploy-to-build.js"
  },
  "keywords": [
    "naimo",
    "plugin",
    "electron",
    "nodejs",
    "npm",
    "npmrc"
  ],
  "author": "Naimo Tools",
  "license": "MIT",
  "dependencies": {
    "vue": "^3.5.22",
    "vue-router": "^4.5.0",
    "pinia": "^2.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^22.0.0",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.5"
  }
}